    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.3.0",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
